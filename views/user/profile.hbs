<div class="row">
    <div class="col-md-8 col-md-offset-2">
        <h1>Brugerprofil</h1>
        <hr>
        <h2>Mine bestillinger</h2>
        {{# each orders }}
            <div class="panel panel-default">
                <div class="panel-body">
                    <ul class="list-group">
                        {{# each this.items }}
                            <li class="list-group-item">
                                <span class="badge">{{ this.price }}</span>
                                <b>{{ this.item.title }} | antal: {{ this.qty }}</b>
                            </li>
                        {{/each}}
                    </ul>
                </div>
                <div class="panel-footer">
                    <strong>Total pris: {{ this.cart.totalPrice }} DKK</strong>
                </div>
            </div>
        {{/each}}
    </div>
</div>